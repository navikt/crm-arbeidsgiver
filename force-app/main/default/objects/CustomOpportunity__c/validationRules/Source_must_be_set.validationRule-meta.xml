<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Source_must_be_set</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK( TEXT ( Source__c ) ), $Profile.Name &lt;&gt; &#39;System Administrator&#39;)</errorConditionFormula>
    <errorDisplayField>Source__c</errorDisplayField>
    <errorMessage>Velg kilden muligheten kommer fra</errorMessage>
</ValidationRule>
