        <delayDays>1</delayDays>
        <description>Velkomstmelding med link til opplæringssider på navet.</description>
        <dismissButtonLabel>Skjul tipset</dismissButtonLabel>
        <displayPosition>TopCenter</displayPosition>
        <displayType>FloatingPanel</displayType>
        <endDate>2020-12-31</endDate>
        <isPublished>true</isPublished>
        <masterLabel>Velkomst melding til NAV Kontor brukere (TAG)</masterLabel>
        <publishedDate>2020-09-11</publishedDate>
        <shouldDisplayActionButton>true</shouldDisplayActionButton>
        <shouldIgnoreGlobalDelay>false</shouldIgnoreGlobalDelay>
        <startDate>2020-09-11</startDate>
        <targetAppDeveloperName>TAG_NAV_default</targetAppDeveloperName>
        <targetPageKey1>home</targetPageKey1>
        <targetPageType>standard__namedPage</targetPageType>
        <timesToDisplay>5</timesToDisplay>
        <title>Velkommen til CRM</title>
