<Prompt xmlns="http://soap.sforce.com/2006/04/metadata">
    <masterLabel>Velkomst melding til NAV Kontor brukere (TAG)</masterLabel>
    <promptVersions>
        <actionButtonLabel>Til Navet</actionButtonLabel>
        <actionButtonLink>https://navno.sharepoint.com/sites/intranett-prosjekter-og-utvikling/SitePages/CRM-l%C3%B8sningen-Salesforce.aspx</actionButtonLink>
