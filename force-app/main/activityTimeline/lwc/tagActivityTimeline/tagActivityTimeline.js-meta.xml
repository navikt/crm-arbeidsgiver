<?xml version="1.0" encoding="UTF-8" ?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
	<apiVersion>47.0</apiVersion>
	<isExposed>true</isExposed>
	<masterLabel>Activity Timeline</masterLabel>
	<description>An activity timeline component that is configurable to 
    multiple objects (including custom objects)</description>
	<targets>
		<target>lightning__RecordPage</target>
	</targets>
	<targetConfigs>
		<targetConfig targets="lightning__RecordPage">
			<property name="headerTitleNorwegian" type="String" label="Header Norwegian (default: Aktiviteter)" />
			<property name="headerTitleEnglish" type="String" label="Header English (default: Activities)" />
			<property name="headerIcon" type="String" label="Header Icon (SLDS Icons)" description="Refer to https://lightningdesignsystem.com/icons" />
			<supportedFormFactors>
				<supportedFormFactor type="Small" />
				<supportedFormFactor type="Large" />
			</supportedFormFactors>
		</targetConfig>
	</targetConfigs>
</LightningComponentBundle>